<script>
    console.log("Edit script loaded and executed.");
    function prepop_and_patch_override(event, data) {

      // pre-populate fields for Edit Container Form
      var modal = $('#editContainer');
      modal.find('#edit-container-id').attr('value', data[event.target.value].container_id);
      modal.find('#edit-container-name').attr('value', data[event.target.value].container_name);
      modal.find('#edit-price-per-unit').attr('value', data[event.target.value].price_per_unit);
      modal.find('#edit-qty-in-stock').attr('value', data[event.target.value].qty_in_stock);
      
      // Override POST with PATCH method
      var form = $('#edit-container-form');
      form.attr('action', '/containers');
      var formAction = form.attr('action');
      form.attr('action', formAction + '/' + data[event.target.value].container_id + '?' + '_method=PATCH');
      }

</script>
  
<script>
    console.log("Delete Script loaded and executed.");
    function delete_override(event, data) {
      var form = $('#delete-container-form');
      form.attr('action', '/containers');
      
      // Override POST with DELETE method
      var formAction = form.attr('action');
      form.attr('action', formAction + '/' + data[event.target.value].container_id + '?' + '_method=DELETE');
      }
</script>